{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "types": ["@cloudflare/workers-types", "vite/client"],
    "module": "ES2022",
    "moduleResolution": "bundler",
    "rootDirs": [".", "./.react-router/types"]
  },
  "include": [
    ".react-router/types/**/*",
    "app/**/*",
    "app/**/.server/**/*",
    "app/**/.client/**/*",
    "workers/**/*"
  ]
}
